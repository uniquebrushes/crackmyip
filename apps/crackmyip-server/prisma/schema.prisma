datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model IpDetails {
  id           String   @id @default(cuid())
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  ipv4         String?
  ipv6         String?
  city         String?
  region       String?
  state        String?
  zipCode      String?
  country      String?
  isp          String?
  organization String?
  asNumber     String?
  asName       String?
  timeZone     String?
  ipAddress    String?
  userAgent    String?
}
